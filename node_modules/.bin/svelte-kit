#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/pebloop/Documents/git/skylang/node_modules/.pnpm/@sveltejs+kit@2.49.2_@sveltejs+vite-plugin-svelte@5.1.1_svelte@5.46.1_vite@6.4.1_jiti@2_36fe1cdbc0792d53b0c42d2d2ec99365/node_modules/@sveltejs/kit/node_modules:/home/pebloop/Documents/git/skylang/node_modules/.pnpm/@sveltejs+kit@2.49.2_@sveltejs+vite-plugin-svelte@5.1.1_svelte@5.46.1_vite@6.4.1_jiti@2_36fe1cdbc0792d53b0c42d2d2ec99365/node_modules/@sveltejs/node_modules:/home/pebloop/Documents/git/skylang/node_modules/.pnpm/@sveltejs+kit@2.49.2_@sveltejs+vite-plugin-svelte@5.1.1_svelte@5.46.1_vite@6.4.1_jiti@2_36fe1cdbc0792d53b0c42d2d2ec99365/node_modules:/home/pebloop/Documents/git/skylang/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/pebloop/Documents/git/skylang/node_modules/.pnpm/@sveltejs+kit@2.49.2_@sveltejs+vite-plugin-svelte@5.1.1_svelte@5.46.1_vite@6.4.1_jiti@2_36fe1cdbc0792d53b0c42d2d2ec99365/node_modules/@sveltejs/kit/node_modules:/home/pebloop/Documents/git/skylang/node_modules/.pnpm/@sveltejs+kit@2.49.2_@sveltejs+vite-plugin-svelte@5.1.1_svelte@5.46.1_vite@6.4.1_jiti@2_36fe1cdbc0792d53b0c42d2d2ec99365/node_modules/@sveltejs/node_modules:/home/pebloop/Documents/git/skylang/node_modules/.pnpm/@sveltejs+kit@2.49.2_@sveltejs+vite-plugin-svelte@5.1.1_svelte@5.46.1_vite@6.4.1_jiti@2_36fe1cdbc0792d53b0c42d2d2ec99365/node_modules:/home/pebloop/Documents/git/skylang/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@sveltejs/kit/svelte-kit.js" "$@"
else
  exec node  "$basedir/../@sveltejs/kit/svelte-kit.js" "$@"
fi
